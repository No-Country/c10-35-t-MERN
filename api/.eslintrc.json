{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "linebreak-style": "off", // regla de saltos de linea, por defecto activa el error con el tipo unix o LF
        "no-unused-vars": "warn"
    }
}
